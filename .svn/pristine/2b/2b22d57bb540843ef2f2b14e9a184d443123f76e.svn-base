#tabs { margin: 0; padding: 0; list-style-type: none; overflow: hidden; border-radius: 3px;border: 2px solid #475050; background: none repeat scroll 0 0 #888888;}
#tabs li { cursor: pointer; float: left; padding: 3px 15px; font-weight: normal; font-size: 12px; background: none repeat scroll 0 0 #888888;color: white;font-size: 14px;}
#tabs li.selected { background: linear-gradient(to bottom, #FCAA40 0%, #D78419 100%) repeat scroll 0 0 transparent;color: #FFFFFF;  font-size: 14px; border-radius: 3px 3px 3px 3px;}
#tabs-content { clear: both; height: 100%;}
#tabs-content.js > div { display: none; }
#tabs-content.js > #tab-content-1 { display: block; }

#propertyTab { margin: 0; padding: 0; list-style-type: none; overflow: hidden; border-radius: 3px;}
#propertyTab li { cursor: pointer; float: left; padding: 3px 9px; font-weight: normal; font-size: 12px;}
#propertyTab li.selected { background: linear-gradient(to bottom, #dadada 0%, #dadada 100%) repeat scroll 0 0 transparent;font-weight: bold;}
#propertyTab-content { clear: both; }
#propertyTab-content.js > div { display: none; }
#propertyTab-content.js > #propertyTab-content-1 { display: block; }

