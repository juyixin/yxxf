	text-align:	right;
}

input.in_ReadOnly {
	text-align:	right;
	border:0px; background:#e0e0e0 ;
}

li img.center {
	display: block;
	margin-left: auto;
	margin-right: auto;
	}
li img.right {
	display: block;
	margin-left: auto;
	margin-right: 10px;
	}
li img.left {
	display: block;
	margin-right: auto;
	margin-left: 10px;
	}
li img.right_m {
	float: right;
	margin: 0 ;
	}
li img.left_m {
	float: left;
	margin: 0;
	}


